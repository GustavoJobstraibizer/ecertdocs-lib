{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-debugger": 0,
    "no-underscore-dangle": "off",
    "lines-between-class-members": "warn"
  },
  "parser": "babel-eslint"
}
